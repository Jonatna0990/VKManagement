<UserControl
    x:Class="VKShop_Lite.UserControls.Video.AddVideoControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VKShop_Lite.UserControls.Video"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" >
    <Grid Background="White" >
        <GridView    x:Name="VideoList" ItemTemplate="{StaticResource VideoTemplate}" SelectionChanged="Selector_OnSelectionChanged" >
            <GridView.Header>
                <TextBlock Margin="5" Text="Добавить видеозапись" Style="{StaticResource TitleTextBlockStyle}"></TextBlock>
            </GridView.Header>
        </GridView>
    </Grid>
</UserControl>
